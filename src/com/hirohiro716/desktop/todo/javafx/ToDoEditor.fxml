<AnchorPane fx:id="paneRoot" prefHeight="800.0" prefWidth="1500.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
    <StackPane AnchorPane.topAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0">
        <VBox fx:id="vboxRoot" spacing="10.0" maxWidth="1300.0" maxHeight="600.0">
            <Label text="TODO" style="-fx-font-size:150%; -fx-font-weight:bold;">
                <padding>
                    <Insets top="10.0" right="10.0" bottom="10.0" left="10.0" />
                </padding>
            </Label>
            <RudeArrayTable fx:id="rudeArrayTable" VBox.vgrow="ALWAYS" />
            <HBox spacing="10.0" alignment="CENTER_RIGHT">
                <ImeOffButton fx:id="buttonAdd" text="追加">
                    <padding>
                        <Insets top="10.0" right="20.0" bottom="10.0" left="20.0" />
                    </padding>
                </ImeOffButton>
                <ImeOffButton fx:id="buttonSort" text="並び替え">
                    <padding>
                        <Insets top="10.0" right="20.0" bottom="10.0" left="20.0" />
                    </padding>
                </ImeOffButton>
                <HBox spacing="10.0" alignment="CENTER_RIGHT" HBox.hgrow="ALWAYS">
                    <ImeOffButton fx:id="buttonSetting" text="設定">
                        <padding>
                            <Insets top="10.0" right="20.0" bottom="10.0" left="20.0" />
                        </padding>
                    </ImeOffButton>
                    <ImeOffButton fx:id="buttonClose" text="終了">
                        <padding>
                            <Insets top="10.0" right="20.0" bottom="10.0" left="20.0" />
                        </padding>
                    </ImeOffButton>
                </HBox>
            </HBox>
            <padding>
                <Insets top="10.0" right="10.0" bottom="10.0" left="10.0" />
            </padding>
        </VBox>
    </StackPane>
</AnchorPane>

